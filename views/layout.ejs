      <% include partials/new-post-dialog %>
    <% } else { %>
      <% include partials/sign-in-dialogs %>
    <% } %>
    
    <script src="/js/app.js"></script>
    <script src="/js/validator.js"></script>
    <script src="/js/jquery.timeago.js"></script>
